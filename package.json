{
  "name": "Cozic-v2",
  "version": "0.0.1",
  "description": "Free & Open Source Music Cloud",
  "license": "MIT",
  "cozy-type": "static",
  "cozy-displayName": "Cozic-v2",
  "cozy-color": "#3BDA6C",
  "icon-path": "main_icon.png",
  "author": "flyingrub",
  "repository": {
    "type": "git",
    "url": "https://github.com/flyingrub/cozy-music.git"
  },
  "dependencies": {
    "backbone": "^1.2.3",
    "backbone.marionette": "^2.4.4",
    "jquery": "^2.2.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-core": "^6.6.5",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "node-sass": "^3.4.2",
    "sass-loader": "^3.1.2",
    "webpack": "^1.12.14"
  },
  "scripts": {
    "build": "env OPTIMIZE=true webpack",
    "watch": "webpack --display-modules --display-chunks --watch",
    "clean": "rm -rf build"
  },
  "cozy-permissions": {
    "File": {
      "description": "Acess music from files."
    },
    "Track": {
      "description": "Hear your track in your cozy."
    },
    "Playlist": {
      "description": "Manage your playlist."
    }
  }
}
